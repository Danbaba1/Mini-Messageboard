<!-- index.ejs -->

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title><%= title %></title>
</head>
<body>
    <% for (let message of messages) { %>
        <ul>
            <li>
                <div>
                      <%= message.added %>
                </div>
                <div>
                     <%= message.user %>
                </div>
                <div>
                    <%= message.text %>
                </div>
            </li>
        </ul>   
    <% } %>
</body>
</html>


